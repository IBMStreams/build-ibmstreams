{
  "name": "build-ibmstreams",
  "main": "./lib/spl-build",
  "version": "0.4.2",
  "description": "IBM Streams build package [Beta]",
  "keywords": [],
  "repository": "https://github.com/IBMStreams/build-ibmstreams",
  "license": "Apache-2.0",
  "scripts": {
    "remotedev": "remotedev --hostname=localhost --port=8000",
    "lint": "eslint **/*.js --ignore-pattern node_modules/ --quiet",
    "pretest": "atom --test spec/preTest.js",
    "test": "atom -t spec/ || atom --test spec/postTest.js",
    "posttest": "atom --test spec/postTest.js"
  },
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "uriHandler": {
    "method": "handleStreamsBuildUri",
    "deferActivation": false
  },
  "consumedServices": {
    "linter-indie": {
      "versions": {
        "2.0.0": "consumeLinter"
      }
    },
    "console": {
      "versions": {
        "0.1.0": "consumeConsoleView"
      }
    },
    "tree-view": {
      "versions": {
        "1.0.0": "consumeTreeView"
      }
    },
    "streams-toolkits": {
      "versions": {
        "1.0.0": "consumeToolkitUpdater"
      }
    }
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.3",
    "@types/archiver": "^2.1.3",
    "@types/atom": "^1.28.0",
    "@types/atom-keymap": "^8.0.3",
    "@types/node": "^11.13.0",
    "@types/request": "^2.47.1",
    "@types/rx-dom": "^7.0.0",
    "@types/rx-node": "^0.0.5",
    "archiver": "^3.0.0",
    "carbon-components": "^10.4.1",
    "carbon-components-react": "^7.4.1",
    "chai": "^4.2.0",
    "clipboardy": "^2.0.0",
    "extract-zip": "^1.6.7",
    "fs-extra": "^8.0.1",
    "immutable": "^4.0.0-rc.12",
    "jszip": "^3.2.1",
    "keytar": "^4.4.1",
    "lodash": "^4.17.13",
    "node-sass": "^4.12.0",
    "prop-types": "^15.6.2",
    "react": "^16.8.6",
    "react-bootstrap": "^1.0.0-beta.6",
    "react-dom": "^16.8.6",
    "react-loading": "^2.0.3",
    "react-redux": "^7.0.1",
    "react-select": "^2.4.2",
    "redux": "^4.0.1",
    "redux-observable": "^1.1.0",
    "request": "^2.88.0",
    "reselect": "^4.0.0",
    "rxjs": "^6.3.2",
    "semver": "^6.0.0",
    "tar-fs": "^2.0.0",
    "xmldoc": "^1.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "babel-register": "6.26.0",
    "electron-devtools-installer": "^2.2.4",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-formatter-pretty": "2.1.1",
    "eslint-import-resolver-webpack": "0.11.0",
    "eslint-plugin-compat": "^3.1.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-mocha": "^5.3.0",
    "eslint-plugin-node": "8.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-react": "7.12.4",
    "eslint-watch": "^5.0.1",
    "redux-devtools-extension": "^2.13.8",
    "remote-redux-devtools": "^0.5.16",
    "remotedev-server": "^0.3.1",
    "sqlite3": "^4.0.7"
  }
}
